{
  "name": "electron-sandbox",
  "productName": "Sandbox",
  "version": "1.0.0",
  "description": "[![Stories in Ready](https://badge.waffle.io/JustinSchuyler/electron-sandbox.png?label=ready&title=Ready)](https://waffle.io/JustinSchuyler/electron-sandbox) # Electron Sandbox ## Mission To learn more about Electron.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "gulp serve",
    "build-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=build",
    "build-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=build",
    "build-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/512x512.png --prune=true --out=build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustinSchuyler/electron-sandbox.git"
  },
  "author": "Rob and Justin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JustinSchuyler/electron-sandbox/issues"
  },
  "homepage": "https://github.com/JustinSchuyler/electron-sandbox#readme",
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-connect": "^0.6.1",
    "electron-packager": "^8.6.0",
    "gulp": "^3.9.1",
    "spectron": "^3.6.1"
  }
}
